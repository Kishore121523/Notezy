{
    "name": "Notezy",
    "version": "0.0.1",
    "description": "Modern Note Taking Extension which let's you organize your notes into pages, update, delete and save notes as pdf",
    "manifest_version": 3,
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "background": {
        "scripts": ["script/modalFunctions.js", "script/script.js", "script/jspdf.min.js"]
    },
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "128.png"
    }

}